<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>

<TableView fx:id="satelliteDetailTable" minHeight="-Infinity" prefHeight="50.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.au.cpsc.satpass.controller.SatelliteDetailTableViewController">
  <columns>
    <TableColumn fx:id="satNoColumn" prefWidth="75.0" text="SatNo" />
    <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
      <TableColumn fx:id="launchYearColumn" prefWidth="75.0" text="Year" />
      <TableColumn fx:id="ildColumn" prefWidth="75.0" text="ILD" />
      <TableColumn fx:id="inclinationColumn" prefWidth="75.0" text="Inc." />
      <TableColumn fx:id="meanMotionColumn" prefWidth="75.0" text="MM" />
      <TableColumn fx:id="eccentricityColumn" prefWidth="75.0" text="Ecc." />
      <TableColumn fx:id="epochColumn" prefWidth="75.0" text="Epoch" />
  </columns>
   <padding>
      <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
   </padding>
</TableView>
